{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "RunLog",
  "type": "object",
  "required": [
    "run_id",
    "status",
    "audit"
  ],
  "properties": {
    "run_id": {
      "type": "string"
    },
    "status": {
      "type": "string",
      "enum": [
        "ok",
        "needs_selection",
        "no_knowledge",
        "refused",
        "error"
      ]
    },
    "missing_selection": {
      "type": "array",
      "items": {
        "type": "string"
      }
    },
    "audit": {
      "type": "object"
    },
    "warnings": {
      "type": "array",
      "items": {
        "type": "string"
      }
    },
    "errors": {
      "type": "array",
      "items": {
        "type": "string"
      }
    }
  },
  "additionalProperties": true
}